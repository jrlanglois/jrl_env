    "defaults": 
    {
        "init.defaultBranch": "main",
        "color.ui": "auto",
        "pull.rebase": "false",
        "push.default": "simple",
        "push.autoSetupRemote": "true",
        "rebase.autoStash": "true",
        "merge.ff": "false"
    },
    "aliases": 
    {
        "st": "status",
        "co": "checkout",
        "br": "branch",
        "ci": "commit",
        "unstage": "reset HEAD --",
        "last": "log -1 HEAD",
        "visual": "!code",
        "log1": "log --oneline",
        "logg": "log --oneline --graph --decorate --all",
        "amend": "commit --amend",
        "uncommit": "reset --soft HEAD^",
        "stash-all": "stash --include-untracked",
        "undo": "reset HEAD~1"
    }
}
